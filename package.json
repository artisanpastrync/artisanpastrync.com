{
    "name": "artisanpastrync.com",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "lint:fix": "next lint --fix",
        "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
        "stripe:listen": "stripe listen --forward-to localhost:3000/api/stripe-webhook",
        "supabase:db:pull": "supabase db pull --linked --schema public,auth,storage",
        "supabase:db:pull:local": "supabase db pull --local --schema public,auth,storage",
        "supabase:db:reset": "supabase db reset",
        "supabase:db:sync": "supabase db dump --linked --data-only -f supabase/seed.sql",
        "supabase:gen-types": "supabase gen types --linked --lang typescript --schema public > src/database.types.ts",
        "supabase:gen-types:local": "supabase gen types --local --lang typescript --schema public > src/database.types.ts",
        "supabase:link": "env-cmd -f .env.local npx supabase link",
        "supabase:restart": "supabase stop && supabase start",
        "supabase:start": "supabase start --ignore-health-check",
        "supabase:status": "supabase status",
        "supabase:stop": "supabase stop"
    },
    "dependencies": {
        "@hookform/resolvers": "^3.9.0",
        "@radix-ui/react-dialog": "^1.1.1",
        "@radix-ui/react-dropdown-menu": "^2.1.1",
        "@radix-ui/react-label": "^2.1.0",
        "@radix-ui/react-progress": "^1.1.0",
        "@radix-ui/react-separator": "^1.1.0",
        "@radix-ui/react-slot": "^1.1.0",
        "@radix-ui/react-tabs": "^1.1.0",
        "@radix-ui/react-tooltip": "^1.1.2",
        "@stripe/react-stripe-js": "^2.8.0",
        "@stripe/stripe-js": "^4.3.0",
        "@supabase/ssr": "^0.4.0",
        "@supabase/supabase-js": "^2.43.5",
        "class-variance-authority": "^0.7.0",
        "clsx": "^2.1.1",
        "lucide-react": "^0.414.0",
        "next": "^14.2.8",
        "next-themes": "^0.3.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-hook-form": "^7.53.0",
        "server-only": "^0.0.1",
        "stripe": "^16.10.0",
        "tailwind-merge": "^2.5.2",
        "tailwindcss-animate": "^1.0.7",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@types/node": "^20",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "autoprefixer": "^10.4.18",
        "env-cmd": "^10.1.0",
        "eslint": "^8.57.0",
        "eslint-config-next": "^14.2.3",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-jsx-a11y": "^6.8.0",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-react": "^7.34.2",
        "eslint-plugin-react-hooks": "^4.6.2",
        "postcss": "^8.4.35",
        "postcss-import": "^16.0.1",
        "prettier": "^3.2.5",
        "supabase": "^1.192.5",
        "tailwindcss": "^3.4.1",
        "typescript": "^5"
    }
}
